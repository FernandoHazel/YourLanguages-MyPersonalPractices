<!DOCTYPE html>
<html lang="en">
    <%- include ('partials/head.ejs') %>
<body class="container">
    <%- include ('partials/header.ejs') %>
    <!--<h1><%= title %></h1>-->

    <%if(professors.length){ %>
        <% professors.forEach(function(professor) { %>
            <div class="panel panel-primary" style="margin: 3px;">
                <div class="panel-heading" style="background-color: rgb(21, 21, 243); color: white; border-radius: 5px;">
                    <%= professor.name %>
                </div>
                <div class="panel-body" style="border-radius: 5px; border: 1px solid #000;">
                    <%= professor.description %>
                </div>
            </div>
       <% }) %>
    <% } %>
        
</body>
</html>